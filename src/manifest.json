{
    "manifest_version": 2,
    "name": "safa的欢迎页 ^_^",
    "version": "1.0",
    "description": "safa的欢迎页 ^_^",
    "background": {
        "scripts": [ "js/background.js" ]
     },
     "chrome_url_overrides" : {
         "newtab": "chromeIndex.html"
     },
    "permissions":[
        "storage",
        "bookmarks",
        "chrome://favicon/",
        "webRequest",
        "webRequestBlocking",
        "*://*/*"
    ],
    "commands": {
        "open-safa-home": {
            "suggested_key": {
                "default": "Ctrl+2",
                "mac": "Command+2"
            },
            "description" : "open-safa-home",
            "global": true
        }
    }
}